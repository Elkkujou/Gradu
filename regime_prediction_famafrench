    {
      "cell_type": "code",
      "source": [
        "if DL == True:\n",
        "    import tensorflow as tf\n",
        "    from tensorflow.keras.models import Sequential\n",
        "    from tensorflow.keras.layers import LSTM, Dense, Dropout\n",
        "    import matplotlib.pyplot as plt\n",
        "    import seaborn as sns\n",
        "    from sklearn.metrics import confusion_matrix\n",
        "    import numpy as np\n",
        "\n",
        "    # Set training window and sequence length to 120 months\n",
        "    training_window = 120  # Number of months used as training data and as sequence length\n",
        "    seq_len = training_window\n",
        "    num_classes = cluster_n  # Assuming regimes are labeled 0, 1, 2\n",
        "    num_features = len(FEATURES)\n",
        "\n",
        "    all_preds = []  # List to store predictions\n",
        "    all_true = []   # List to store true regime labels\n",
        "    dates_list = [] # List to store the corresponding dates\n",
        "\n",
        "    # Ensure data is sorted by date\n",
        "    data = input_df.sort_values(DATE).reset_index(drop=True)\n",
        "\n",
        "    # Rolling window: start from index = training_window until the end of data\n",
        "    for i in range(training_window, len(data)):\n",
        "        # Use the previous 120 months as the training window\n",
        "        train_window = data.iloc[i - training_window:i].reset_index(drop=True)\n",
        "        # Ensure we have a full training window\n",
        "        if len(train_window) < training_window:\n",
        "            continue\n",
        "\n",
        "        # Create one training sample: the entire 120-month window becomes one sequence\n",
        "        X_train_seq = train_window[FEATURES].values.reshape(1, seq_len, num_features)\n",
        "        # Use the regime value at the end of the training window as the target for training\n",
        "        y_train_seq = np.array([train_window[TRUE_REGIME].iloc[-1]])  # Convert to NumPy array\n",
        "\n",
        "        # Build a simple LSTM model\n",
        "        model = Sequential([\n",
        "            LSTM(32, input_shape=(seq_len, num_features)),\n",
        "            Dropout(0.2),\n",
        "            Dense(num_classes, activation='softmax')\n",
        "        ])\n",
        "        model.compile(loss='sparse_categorical_crossentropy', optimizer='adam', metrics=['accuracy'])\n",
        "\n",
        "        # Train the model on the single training sample (epochs can be adjusted)\n",
        "        model.fit(X_train_seq, y_train_seq, epochs=10, batch_size=1, verbose=0)\n",
        "\n",
        "        # For prediction, we use the same training window as input\n",
        "        pred_prob = model.predict(X_train_seq, verbose=0)\n",
        "        pred_regime = np.argmax(pred_prob, axis=1)[0]\n",
        "\n",
        "        # The true regime for comparison is the regime value at time index i (next month)\n",
        "        true_regime = data[TRUE_REGIME].iloc[i]\n",
        "\n",
        "        all_preds.append(pred_regime)\n",
        "        all_true.append(true_regime)\n",
        "        dates_list.append(data[DATE].iloc[i])\n",
        "\n",
        "        print(f\"Prediction for {data[DATE].iloc[i]}: Predicted={pred_regime}, True={true_regime}\")\n",
        "\n",
        "    # Create and visualize the confusion matrix\n",
        "    cm = confusion_matrix(all_true, all_preds)\n",
        "    plt.figure(figsize=(6, 4))\n",
        "    sns.heatmap(cm, annot=True, fmt='d', cmap='Blues')\n",
        "    plt.title(\"Rolling Window LSTM Confusion Matrix\")\n",
        "    plt.xlabel(\"Predicted\")\n",
        "    plt.ylabel(\"True\")\n",
        "    plt.show()\n"
      ],
      "metadata": {
        "id": "DoyxwlWcFY1z"
      },
      "execution_count": null,
      "outputs": []
    },
